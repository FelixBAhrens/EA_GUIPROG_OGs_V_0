<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="BOTTOM_LEFT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.ea_guiprog_ogs_v_0.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Fortfahren" onAction="#onFortfahrenButtonClick"/>
    <Button text="Neues Spiel" onAction="#onNeuesSpielButtonClick"/>
    <Button text="test" onAction="#onBeendenButtonClick"/>

</VBox>
